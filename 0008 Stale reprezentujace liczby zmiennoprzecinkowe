#include <iostream>
using namespace std;
int main()
{
	double promien = 1.7;
	double pole = promien * promien * 3.14;
	cout << "\nPole kola o promieniu" << promien << " wynosi\n" << pole;

	promien - 4.1e2;
	pole - promien * promien * 3.14;
	cout << "\nPole kola o promieniu" << promien << " wynosi\n" << pole;
}
